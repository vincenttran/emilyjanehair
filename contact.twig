{% extends "layout.twig" %}

{% block content %}
<div class="bk-pt-accessories">
    <header class="bk-header">
        <div class="header-bg">
             <div class="wrapper companyheader clearfix">
                {{widget('logo', 'logo', {"defaultlogo": asset('images/logo.png')})|raw}}
            </div>
        </div>
        <nav class="navbar clearfix">
           {{widget('extendednavigation', 'navigationheader', {})|raw}}
        </nav>
    </header>
    
<div class="accessoriesimage">
</div>
    
    <div class="main-accessories clearfix">
        <div class="responsivecolumns columns-two-50-50 wrapper">
            <div class="column columns-two-left">
                    <div class="box-accessories">
                        {{widget('content', 'textwidgetbox6', {content: '<p>ACCESSORIES</p>', lines:    'all'})|raw}}
                        {{widget('content', 'textwidgetbox6', {content: '<p>Whether its for Festival    Season or another occasion we can apply all types of feathers, flowers, beads and  tinsel strands into your hair.</p>', lines: 'all'})|raw}}
                        {{widget('content', 'textwidgetbox7', {content: '<p>Call us now for a FREE  consultation to discuss what you would like and to have a look at our fabulous   hair accessories range.</p>', lines: 'all'})|raw}}
                    <div class="feather">
                        {{widget('image', 'thisunqiuewidgetname2', { 'src': asset('emilyjaneextensions/ images/feather.jpg'), 'title': 'my image', 'alt': 'my image', 'fileName': 'feather   .jpg', 'fileSize': '58kb', 'imageSize': '440 x 660', 'imageWidth': '90px'})|raw}}
                    </div>
                </div>
            </div>
            <div class="column columns-two-right">
                <div class="accessories-img">
                </div>
            </div>
        </div>
    </div>  
    
    <footer class="bk-footer">
        <div class="wrapper clearfix">
            <div class="responsivecolumns columns-two-50-50">
                <div class="column columns-two-left">
                    {{widget('profile', 'socialicons', { 'profileType':'socialicons',   'socialicons':'profile'})|raw}}
                </div>
                <div class="column columns-two-right">
                    {{widget('profile', 'phone', { 'profileType':'phone', 'phone':'profile',    'refreshtime':'1800000'})|raw}}
                </div>
            </div>
        </div>
    </footer>
</div>
{% endblock %}